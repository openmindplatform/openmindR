key <- unserialize(sodium::data_decrypt(bin = decipher, key = omkey, nonce = public_nonce))
# message("Setting API key as AIRTABLE_API_KEY environment variable.")
Sys.setenv(AIRTABLE_API_KEY = key)
return(key)
}
